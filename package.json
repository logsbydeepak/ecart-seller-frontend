    "postcss": "^8.4.14",
    "prettier": "^2.7.1",
    "prettier-plugin-tailwindcss": "^0.1.12",
    "rimraf": "^3.0.2",
    "tailwindcss": "^3.1.6",
    "typescript": "4.7.4"
